<style>
    .main_box_order {
        width: 100%;
        height: 100%;

        display: flex;
        align-items: center;
        justify-content: center;
        padding: 1rem 0rem;
    }

    .sub_main_order_div {
        width: 95%;
        height: 100%;

    }

    .main_heading_text h2 {
        color: #000;
        font-family: Be Vietnam Pro;
        font-size: 25px;
        font-style: normal;
        font-weight: 600;
        line-height: normal;
    }

    .ineer_order_details {
        width: 100%;
        height: 100%;

        padding: 2rem;
        display: flex;
        align-items: center;
        justify-content: flex-start;
        gap: 2rem;
        border-bottom: 1px solid black;
        position: relative;
    }

    .date_mainbox {
        width: 10%;
        height: 100%;

        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
    }

    .date_mainbox span {
        color: #000;
        text-align: center;
        font-family: Be Vietnam Pro;
        font-size: 18px;
        font-style: normal;
        font-weight: 500;
        line-height: normal;
    }

    .date_mainbox h1 {
        text-align: center;
        color: #000;
        leading-trim: both;
        text-edge: cap;
        font-family: Be Vietnam Pro;
        font-size: 40px;
        font-style: normal;
        font-weight: 700;
        line-height: 41px;
        /* 91.111% */
        letter-spacing: 1.8px;

    }

    .order_details_text {
        padding: 10px;
        width: 100%;
        height: 100%;

    }

    .lunch_div {
        display: flex;
        align-items: flex-start;
        justify-content: flex-start;
        gap: 2rem;

    }

    .list_heading_div {
        width: 100px;
    }

    .lunch_div h3 {
        color: #000;
        font-family: Be Vietnam Pro;
        font-size: 18px;
        font-style: normal;
        font-weight: 700;
        line-height: normal;
    }

    .btn_view {
        position: absolute;
        right: 30px;
        bottom: 31px;

    }

    .btn_view_details {
        border: none;
        outline: none;
        padding: 5px 15px;
        align-items: center;
        gap: 10px;
        flex-shrink: 0;
        border-radius: 6px;
        background: #FD3E6C;
        color: #FFF;
        font-family: Be Vietnam Pro;
        font-size: 14px;
        font-style: normal;
        font-weight: 700;
        line-height: normal;
        z-index: 9999;
    }
</style>

</style>

